sudo apt-get install git subversion meld//安装git subversion meld
mkdir workspace
cd workspace/
 mkdir test
cd test/
 git init
touch a
git add .
git commit -m 'a'
git config --global user.email "ye.chen@supwisdom.com"
git config --global user.name "ye.chen"
git commit -m 'a'
 git log 
 echo 'adfsdfs'>a
 git add .
 git commit -m 'aa'
 git log
 git difftool HEAD^!
 sudo apt-get install git-flow//安装git flow
 ls
 git flow init
 git branch 
 git flow feature start b1
 echo 'sdfsdfsd' >> a
 git add .
git commit -m 'sdfsdf'
git checkout develop 
 git flow feature start b2
 cat a
 echo '22343443' >> 'a'
 git add .
 git commit -m 'sfdsfsdf'
 cat a
 git branch 
 git checkout develop 
 git flow feature finish b1
git flow feature finish b2
 git mergetool



